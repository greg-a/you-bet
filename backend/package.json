    "express": "^4.18.1",
    "if-env": "^1.0.4",
    "jest": "^28.1.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.20.1",
    "supertest": "^6.2.4"
  },
  "keywords": [],
